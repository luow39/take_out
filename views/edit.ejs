  <body class="text-center">

<main class="form-signin w-100 m-auto">
    <form action="/user/edit" class="form col-md-12 center-block" method="post">
        <h1 class="h3 mb-3 fw-normal">请修改您的信息</h1>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-default">ID</span>
            </div>
            <input class="form-control" id="disabledInput" type="text" placeholder=<%=data['id'] %> disabled>
        </div>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-default">密码</span>
            </div>
            <input type="password" class="form-control" aria-label="Default" name="passwd" aria-describedby="inputGroup-sizing-default">
        </div>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-default">用户名</span>
            </div>
            <input type="text" class="form-control" aria-label="Default" name="name" placeholder=<%=data['name'] %>>
        </div>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-default">电话</span>
            </div>
            <input type="text" class="form-control" aria-label="Default" name="phone" placeholder=<%=data['phone'] %>>
        </div>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-default">地址</span>
            </div>
            <input type="text" class="form-control" aria-label="Default" name="address" placeholder=<%=data['address'] %>>
        </div>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <span class="input-group-text" id="inputGroup-sizing-default">起送价</span>
            </div>
            <input type="text" class="form-control" aria-label="Default" name="fprice" placeholder="商家需填写">
        </div>


        <button class="w-100 btn btn-lg btn-primary" type="submit">确认修改</button>
    </form>
</main>



  </body>
